{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "airbnb",
	"plugins": [
		"react"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"parser": "babel-eslint",
	"globals": {
		"$": true,
		"S3IMAGEPATH": true
	},
	"rules": {
		"indent": [
			"error",
			"tab", {
				"SwitchCase": 1
			}
		],
		"quotes": [
			"error",
			"single", {
				"allowTemplateLiterals": true
			}
		],
		"semi": [
			"error",
			"always"
		],
		"linebreak-style": 0,
		"no-console": 0,
		"no-tabs": 0,
		"import/no-unresolved": 0,
		"no-underscore-dangle": 0,
		"import/extensions": 0,
		"import/no-extraneous-dependencies": 0,
		"react/jsx-filename-extension": [1, {
			"extensions": [".js", ".jsx"]
		}],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"react/forbid-prop-types": "off"
	}
}